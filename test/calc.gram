expr:
    | expr '+' term { BinOp(expr, '+', term) }
    | expr '-' term { BinOp(expr, '-', term) }
    | term

term:
    | term '*' unary { BinOp(term, '*', unary) }
    | term '/' unary { BinOp(term, '/', unary) }
    | unary

unary:
    | '+' unary { UnaryOp('+', unary) }
    | '-' unary { UnaryOp('-', unary) }
    | atom

atom:
    | NAME
    | NUMBER
    | '(' expr ')' { expr }

